SELECT * FROM casa_campaign

SELECT
    custid,
    SUM(N01 + N02 + N03 + N04 + N05 + N06 + N07 + N08 + N09 +
        N10 + N11 + N12 + N13 + N14 + N15 + N16 + N17 + N18 +
        N19 + N20 + N21 + N22 + N23 + N24 + N25 + N26 + N27 +
        N28 + N29 + N30 + N31) AS 'tá»•ng'
FROM casa_campaign
GROUP BY custid
